{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "birthdays-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/birthdays/BirthdaysWebPart.js",
          "manifest": "./src/webparts/birthdays/BirthdaysWebPart.manifest.json"
        }
      ]
    },
    "finder-wp-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/finderWp/FinderWpWebPart.js",
          "manifest": "./src/webparts/finderWp/FinderWpWebPart.manifest.json"
        }
      ]
    },
    "calender-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/calender/CalenderWebPart.js",
          "manifest": "./src/webparts/calender/CalenderWebPart.manifest.json"
        }
      ]
    },
    "znews-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/znews/ZnewsWebPart.js",
          "manifest": "./src/webparts/znews/ZnewsWebPart.manifest.json"
        }
      ]
    },
    "quick-links-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/quickLinks/QuickLinksWebPart.js",
          "manifest": "./src/webparts/quickLinks/QuickLinksWebPart.manifest.json"
        }
      ]
    },
    "welcome-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/welcome/WelcomeWebPart.js",
          "manifest": "./src/webparts/welcome/WelcomeWebPart.manifest.json"
        }
      ]
    },
    "welcome-employee-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/welcomeEmployee/WelcomeEmployeeWebPart.js",
          "manifest": "./src/webparts/welcomeEmployee/WelcomeEmployeeWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "BirthdaysWebPartStrings": "lib/webparts/birthdays/loc/{locale}.js",
    "CalenderWebPartStrings": "lib/webparts/calender/loc/{locale}.js",
    "ZnewsWebPartStrings": "lib/webparts/znews/loc/{locale}.js",
    "PropertyControlStrings": "node_modules/@pnp/spfx-property-controls/lib/loc/{locale}.js",
    "QuickLinksWebPartStrings": "lib/webparts/quickLinks/loc/{locale}.js",
    "FinderWpWebPartStrings": "lib/webparts/finderWp/loc/{locale}.js",
    "WelcomeWebPartStrings": "lib/webparts/welcome/loc/{locale}.js",
    "WelcomeEmployeeWebPartStrings": "lib/webparts/welcomeEmployee/loc/{locale}.js"
  }
}
