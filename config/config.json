{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "birthdays-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/birthdays/BirthdaysWebPart.js",
          "manifest": "./src/webparts/birthdays/BirthdaysWebPart.manifest.json"
        }
      ]
    },
    "finder-wp-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/finderWp/FinderWpWebPart.js",
          "manifest": "./src/webparts/finderWp/FinderWpWebPart.manifest.json"
        }
      ]
    },
    "calender-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/calender/CalenderWebPart.js",
          "manifest": "./src/webparts/calender/CalenderWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "BirthdaysWebPartStrings": "lib/webparts/birthdays/loc/{locale}.js",
    "FinderWpWebPartStrings": "lib/webparts/finderWp/loc/{locale}.js",
    "CalenderWebPartStrings": "lib/webparts/calender/loc/{locale}.js",
    "PropertyControlStrings": "node_modules/@pnp/spfx-property-controls/lib/loc/{locale}.js"
  }
}